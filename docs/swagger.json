{
  "openapi": "3.0.4",
  "info": {
    "title": "HosDemo.Api",
    "version": "1.0"
  },
  "paths": {
    "/auth/login": {
      "post": {
        "tags": [
          "Auth"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/VendorLoginRequest"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/VendorLoginRequest"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/VendorLoginRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/auth/logout": {
      "post": {
        "tags": [
          "Auth"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LogoutRequest"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/LogoutRequest"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/LogoutRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/drivers/{id}/hos": {
      "get": {
        "tags": [
          "Drivers"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/eld/events": {
      "post": {
        "tags": [
          "Eld"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/EldEventBatch"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/EldEventBatch"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/EldEventBatch"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "EldEventBatch": {
        "type": "object",
        "properties": {
          "vendorId": {
            "type": "string",
            "nullable": true
          },
          "events": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/UpdateDriverHos"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "LogoutRequest": {
        "type": "object",
        "properties": {
          "sessionToken": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "UpdateDriverHos": {
        "type": "object",
        "properties": {
          "driverId": {
            "type": "string",
            "format": "uuid"
          },
          "availableHours": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "availableDrivingTime": {
            "type": "number",
            "format": "double",
            "nullable": true
          },
          "availableOnDutyTime": {
            "type": "number",
            "format": "double",
            "nullable": true
          },
          "available6070": {
            "type": "number",
            "format": "double",
            "nullable": true
          },
          "dutyStatus": {
            "type": "string",
            "nullable": true
          },
          "recordedAt": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      },
      "VendorLoginRequest": {
        "type": "object",
        "properties": {
          "vendorId": {
            "type": "string",
            "nullable": true
          },
          "username": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}